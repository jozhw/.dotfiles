# git lfs manage all pdf files
*.pdf filter=lfs diff=lfs merge=lfs -text
# git lfs manage all png files
*.png filter=lfs diff=lfs merge=lfs -text
*.tar filter=lfs diff=lfs merge=lfs -text
.emacs.d/elpa/[a-zA-Z0-9]+-[0-9.]+/**/* filter=lfs diff=lfs merge=lfs -text
!.emacs.d/elpa/[a-zA-Z0-9]+-[0-9.]+/**/*.elc !filter !diff !merge
!.emacs.d/elpa/[a-zA-Z0-9]+-[0-9.]+/**/*.signed !filter !diff !merge
