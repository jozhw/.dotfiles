{
  "framework": "hugo",
  "build": {
    "env": {
      "HUGO_VERSION": "0.135.0",
      "GO_VERSION": "1.23"
    }
  },
  "buildCommand": "cd docs && hugo --gc --minify",
  "installCommand": "yum install golang && cd docs && hugo mod get -u && go mod download",
  "outputDirectory": "docs/public"
}
